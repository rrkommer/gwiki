<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="gwikiplugin" class="de.micromata.genome.gwiki.plugin.GWikiPluginDescriptor">
		<property name="apiVersion" value="1.0.0" />
		<property name="name" value="gwiki.blog" />
		<property name="version" value="1.0.0" />
    <property name="versionState" value="Beta" />
		<property name="description" value="Blogging Plugin for GWiki" />
		<property name="author" value="Roger Kommer"/>
		<property name="license" value="Apache License, Version 2.0"/>
		<property name="category" value="Social Network"/>
    <property name="descriptionPath" value="pub/plugins/gwiki-blog/Index"/>
	</bean>
</beans>