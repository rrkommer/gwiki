<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="gwikiplugin" class="de.micromata.genome.gwiki.plugin.GWikiPluginDescriptor">
		<property name="apiVersion" value="1.0.0" />
		<property name="name" value="gwiki.fssvn" />
		<property name="version" value="1.0.0" />
    <property name="versionState" value="Alpha" />
		<property name="description" value="WebDAV/SVN Virtual File System. This plugin uses the Sardine WebDAV-Client" />
		<property name="author" value="Roger Rene Kommer and http://code.google.com/p/sardine" />
		<property name="license" value="Apache License 2.0"/>
		<property name="category" value="System" />
		<property name="requiredPlugins">
			<list>
				<value>org.apache.httpcomponents:4.1.0</value>
			</list>
		</property>
	</bean>
</beans>