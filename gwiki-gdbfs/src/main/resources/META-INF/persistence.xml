<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
    <persistence-unit name="de.micromata.genome.gdbfs.jpa" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
    <non-jta-data-source>java:/comp/env/genome/jdbc/dsWeb</non-jta-data-source>
    <class>de.micromata.genome.gdbfs.jpa.JpaFilesystemDO</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
